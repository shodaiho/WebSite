<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
	
<struts>
	<package name="WebSite" extends="struts-default">
		<action name="hello" class="com.test.Action.HelloAction">
			<result name="success">/WEB-INF/jsp/login.jsp</result>
		</action>
		
		<action name="login" class="com.test.Action.LoginAction" method="login">
			<result name="success">/WEB-INF/jsp/addBookMark.jsp</result>
			<result name="error">/login.jsp</result>
		</action>
	</package>
</struts>